{
  "from": "{startingDocumentIndex}",
  "query": {
    "bool": {
      "should": [],
      "must_not": [],
      "must": [
        {
          "term": {
            "ukprn": {
              "value" : "{ukprn}"
            }
          }
        }
      ]
    }
  },
  "size": "{pageSize}",
  "sort": [
    {
      "closingDate": {
        "order": "asc"
      }
    }
  ]
}